# warp2api - environment example (SSOT token pool mode)

# Server
HOST=127.0.0.1
PORT=28889

# External API auth
API_TOKEN=change_me_to_secure_token

# Admin auth: off / local / token
WARP_ADMIN_AUTH_MODE=off
# ADMIN_TOKEN=change_me_admin_token

# Token pool storage (optional)
# WARP_TOKEN_DB_PATH=/absolute/path/to/token_pool.db

# Retry and cooldown
WARP_REQUEST_RETRY_COUNT=3
WARP_REQUEST_RETRY_BASE_DELAY_MS=300
WARP_TOKEN_REFRESH_RETRY_COUNT=3
WARP_TOKEN_REFRESH_RETRY_BASE_DELAY_MS=400
WARP_TOKEN_ERROR_COOLDOWN_SECONDS=180
WARP_TOKEN_COOLDOWN_SECONDS=600
WARP_TOKEN_UNHEALTHY_FAILURE_THRESHOLD=3

# Monitor
WARP_POOL_MONITOR_INTERVAL_SECONDS=120
WARP_POOL_TOKEN_REFRESH_INTERVAL_SECONDS=180
WARP_POOL_MONITOR_MAX_PARALLEL=3
WARP_POOL_QUOTA_RETRY_LEAD_SECONDS=300

# Optional transport/client identity
# WARP_CLIENT_VERSION=v0.2026.02.11.08.23.stable_02
# WARP_OS_CATEGORY=macOS
# WARP_OS_NAME=macOS
# WARP_OS_VERSION=26.4

# Optional Google endpoints for refresh flow (usually keep defaults)
# WARP_REFRESH_URL=https://app.warp.dev/proxy/token?key=AIza...
# WARP_SECURETOKEN_URL=https://securetoken.googleapis.com/v1/token

# Keep startup warmup disabled in unified mode
WARP_COMPAT_STARTUP_WARMUP=false

# Legacy/compat JWT variables are not required in SSOT mode:
# WARP_JWT, WARP_REFRESH_TOKEN, WARP_REFRESH_TOKEN_B64
